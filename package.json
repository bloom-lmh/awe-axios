  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "description": "A lightweight, annotation-driven framework for enhancing Axios with declarative API definitions and powerful features.",
  "files": [
    "dist"
  ],
  "keywords": [
    "axios",
    "msw",
    "api"
  ],
  "author": "SWUST-LMH <1357526355@qq.com>",
  "license": "MIT",
  "buildOptions": {
